{
  "name": "magic-paste-gui",
  "version": "1.0.0",
  "description": "Electron shell for the Magic Paste GUI prototype.",
  "main": "main.js",
  "packageManager": "pnpm@10.26.0",
  "type": "commonjs",
  "scripts": {
